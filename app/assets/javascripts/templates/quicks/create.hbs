<p class="lead">
  Selected Apps
</p>
<table class="table" id='quickSearchResultTable'>
  <tbody>
    {{#each apps}}
      <tr data-id="{{this.id}}">
        <td class="valign-middle">
          {{> quicks/_app_icon}}
        </td>
        <td class="valign-middle">
          <img src="{{this.logo_url}}" alt="{{this.title}}" width="64">
        </td>
        <td class="valign-middle">
          <h4>
          <a href="{{this.url}}" target="_blank">{{this.title}}</a>
          <br>
          <span class="label label-info">{{this.publisher}}</span>
          </h4>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
<p class="lead">
  Access Code
  <input type="text" value="{{access.code}}"/>
</p>
<p class="lead">
  Link to publish
  <input type="text" value="{{access.link}}"/>
</p>
<p class="lead">
  Button code
  <textarea>{{access.button}}</textarea>
</p>
<span class="btn btn-primary" id="actionQuickSave">Save</span>
<span class="btn btn-default" id="actionQuickCancel">Reset</span>
